{
  "name": "multiplayer-service",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "commit": "npx cz",
    "prepare": "husky",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint src/**/*.ts",
    "dev:local": "NODE_ENV=local AUTH_STRATEGY=signature npx nodemon",
    "dev:local:no-auth": "NODE_ENV=local AUTH_STRATEGY=dev npx nodemon",
    "start:goerli": "NODE_ENV=goerli npx nodemon",
    "start:sepolia": "NODE_ENV=sepolia npx nodemon",
    "check": "tsc --noEmit",
    "build": "npm run build:contracts && tsc",
    "build:contracts": "typechain --target=ethers-v6 --out-dir ./src/generated/contracts ./src/abi/**.json",
    "start": "npm run build && node dist/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "ethers": "^6.11.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "graphql-request": "^6.1.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "winston": "^3.11.0"
  }
}
