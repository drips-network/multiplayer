{
  "name": "multiplayer-service",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "commit": "npx cz",
    "prepare": "husky",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint src/**/*.ts",
    "dev": "AUTH_STRATEGY=signature npx nodemon",
    "dev:no-auth": "AUTH_STRATEGY=dev npx nodemon",
    "check": "tsc --noEmit",
    "build": "npm run build:graphql && npm run build:contracts && tsc",
    "build:contracts": "typechain --target=ethers-v6 --out-dir ./src/generated/contracts ./src/abi/**.json",
    "build:graphql": "./scripts/build-gql-types.sh",
    "start": "npm run build && node dist/src/main.js",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "codegen": "graphql-codegen --config codegen.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@safe-global/api-kit": "^2.3.0",
    "@safe-global/protocol-kit": "^3.0.1",
    "@safe-global/safe-core-sdk-types": "^4.0.1",
    "dotenv": "^16.4.5",
    "ethers": "^6.11.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "graphql-request": "^6.1.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "winston": "^3.11.0"
  }
}
